<h1>Here are some Artists!</h1> 
<ul style='columns: 4'>
  <% media.each do |medium| %>
    <li><%= link_to medium.title, medium_path(medium.medium) %></li>
  <% end %>
</ul>
<% if logged_in? %>
  <%= render '/media/prompt_new', medium_name: media.first.class.name, new_path: new_path %>
<% else %>
  Log in or Sign up to add an Artist!
<% end %>