<%= form_for review do |f| %>
    <%= f.hidden_field :medium_id, value: medium_id %>
    <%= f.hidden_field :user_id, value: current_user.id %>
    <%= f.label :content, 'Content:' %>
    <%= f.text_field :content %>
    <%= f.label :rating, 'Rating:' %>
    <%= f.select :rating, (1..5) %>
    <%= f.submit %>
<% end %>