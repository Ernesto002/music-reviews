<h1>Log In!</h1>
<p id="notice">
  <%= notice %>
</p>

<%= form_for @user, url: '/login' do |f| %>
  <%= f.label :username, 'User Name:' %><br>
  <%= f.text_field :username %><br>
  <%= f.label :password, 'Password:' %><br>
  <%= f.password_field :password %><br>
  <%= f.submit 'Log In!'%>
<% end %>