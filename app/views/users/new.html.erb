<h1>Sign Up!</h1>
<p id="notice">
  <%= notice %>
</p>

<%= form_for @user, url: '/login' do |f| %>
    <%= f.label :username, 'User Name:' %><br>
    <%= f.text_field :username %><br>
    <%= f.label :email, 'Email:' %><br>
    <%= f.text_field :email %><br>
    <%= f.label :password, 'Password:' %><br>
    <%= f.password_field :password %><br>
    <%= f.submit 'Sign Up!'%>
<% end %>